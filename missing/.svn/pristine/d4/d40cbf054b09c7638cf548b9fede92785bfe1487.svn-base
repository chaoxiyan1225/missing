package happylife.util.requestandresponse.messagebean;

import java.io.Serializable;

/** 返回给微信的 消息体
 * @author 闫朝喜
 *
 * by 2016-5-21 
 */
public class ResultMsgBean implements Serializable {

	private static final long serialVersionUID = 1L;
	private boolean isResultOk = true;
	private String resultMsg;
	private int currentPage ;

	public ResultMsgBean() {
	}

	public ResultMsgBean(boolean isResultOk, String resultMsg) {
		this.isResultOk = isResultOk;
		this.resultMsg = resultMsg;
	}

	public boolean getIsResultOk() {
		return isResultOk;
	}

	public void setIsResultOk(boolean isResultOk) {
		this.isResultOk = isResultOk;
	}

	public String getResultMsg() {
		return resultMsg;
	}

	public void setResultMsg(String resultMsg) {
		this.resultMsg = resultMsg;
	}

	public int getCurrentPage() {
		return currentPage;
	}

	public void setCurrentPage(int currentPage) {
		this.currentPage = currentPage;
	}
	
}
