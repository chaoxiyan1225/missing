.seaBurial_header{
    width: 100%;
    line-height: 48px;
    font-size: 20px;
    color: #333;
    text-align: center;
    position: relative;
    border-bottom: 1px solid #e5e5e5;
}
/**/
.diamond_bgBox{ width:100%; }
.splitLine{ display:block; width:100%; height:1px; background-color:#ccc; border-bottom:1px solid #fff; margin:50px 0;}
/*定制专属*/
.diamond_profile{ padding:0 10px;}
.diamond_title{ text-align:center; font-size:26px; color:#333; font-weight:normal; margin:40px 0;}
.profile_content{ padding:0; margin:0; overflow:hidden;}
.profile_contentOne{ list-style:none; width:100%; float:left;}
.profile_contentOne img{
    display: block;
    margin: 0 auto;
    width: 110px;
    margin-bottom: 10px;
}
.profile_contentOne02{ list-style:none; padding: 30px 30px 10px;}
.profile_contentOne02 img{ width:45%; height:auto; margin:auto; display:block;}
h1.h1-box{
    font-size: 25px;
    text-align: center;
    color: #795140;
    font-weight: inherit;
    margin-top: 16px;
    line-height: 18px;
    margin-bottom: 20px;
}
h1.h1-box span{
    display: inline-block;
    width: 40px;
    border-bottom: 4px solid #795140;
}
.conten_title{ margin:0; font-size:20px; color:#333; margin-bottom:10px; text-align:center;}
.conten_text{
    font-size: 14px;
    line-height: 22px;
    width: 90%;
    margin: 0 auto;
    text-align: center;
    color: #505050;
}

/*我要定制*/
.diamond_make{ /*padding:0 6px;*/}
.step_title,
.step_title_span,
.miniType,.stepRight_02,.stepRight_03,.callNum,
.stepRight ul,.stepRight span{
    padding: 0 10px;
}
.stepRight ul li{
    list-style: none;
}
.make_title{ text-align:center; font-size:14px; color:#333; margin:0; font-weight:normal;}
.make_stepBox{ overflow:hidden; width:100%; margin:20px auto; padding:0;}
.make_step{font-size: .3rem; height: 58px;line-height: 45px; color: #795140;text-align: center; list-style:none; width: 265px; margin:0 auto; background: url("../img/diamond/bg-li.png"/*tpa=http://m.yiko.org/mobile/img/diamond/bg-li.png*/) 0 0 no-repeat;background-size: 100% 100%;margin-bottom: 10px;}
.make_step5{
    height: 46px;
    background: url("../img/diamond/bg-li2.png"/*tpa=http://m.yiko.org/mobile/img/diamond/bg-li2.png*/) 0 0 no-repeat;
    background-size: 100% 100%;
}
/*1*/
.step_title{ font-size:16px; color:#333;}
.step_title span{ display:block; font-size:14px; color:#666; margin-bottom:20px; font-weight:normal; margin-left:10px;}
.step_title_span{ display:block; font-size:14px; color:#666; margin-bottom:10px; font-weight:normal;}

/*迷你优惠*/
.miniBox{ margin:10px 0; margin-bottom:40px;}
.miniType{ font-size:14px; color:#333; line-height:26px; margin-bottom:10px;}
.miniType img{ display:block; float:left; height:26px; width:auto; margin-right:4px;}
.miniForm{ margin:0; padding:0; overflow:hidden;}
.miniForm li{ list-style:none; float:left; width: 23%;border-right: 1px solid #ccc;}
.miniForm li:last-child{
    border: none;
}
.miniForm li span{ display:block; text-align:center; font-size:14px; color:#333; background-color:#cccccc; line-height:30px;}
.miniForm li a{ border-bottom: 1px solid #ccc;display:block; font-size:14px; color:#333; text-align:center; line-height:30px; text-decoration:none; background-color:#fff;}
.miniForm li div{ border-bottom: 1px solid #ccc;display:block; font-size:14px; color:#333; text-align:center; line-height:30px; background-color:#fff;}


/**/
.make_price{ margin:0; padding:0; background: #f3f3f3;padding-bottom: 18px;background: #f3f3f3;}
.makePrice_line{ list-style:none; margin-bottom:1px;}
.makePrice_column{ width:100%; overflow:hidden; margin:0; padding:0;}
.makePrice_column li{ list-style:none; float:left;
    width:27.6%;/*写给不支持calc()的浏览器*/
    width:-moz-calc(28% - 1px);
    width:-webkit-calc(28% - 1px);
    width: calc(28% - 1px);
    margin-left:1px; background-color:#fff; text-align:center; font-size:16px; color:#333; padding:16px 0 4px 0; font-weight:bolder;}
.makePrice_column .makePrice_header{ background-color:#fafafa; height:70px;}
.makePrice_header img{ display:block; width:30px; height:30px; margin:auto; margin-bottom:4px;}
.makePrice_header span{ display:block; font-size:16px; font-weight:bolder; color:#333;}
.makePrice_headerTitle span{ display:block; font-size:14px; line-height:20px; font-weight:normal; color:#666; margin-top:4px;}
.makePrice_column .makePrice_headerTitle{ width:16%; padding-top:24px; padding-bottom:18px; background-color:#fafafa; margin-left:0;}


.makePrice_column .column_Title{ width:16%; background-color:#fafafa; margin-left:0;}
.content_text{ font-size:16px; color:#333; text-align:center; height:40px; padding-bottom:10px; line-height:20px;}
.makePrice_column li{ text-align:center; position:relative;}
.price_num{ width:100%; font-size:16px; color:#333; margin:0; line-height:30px;}
.price_shape{ display:block; height:20px; width:auto; margin:auto;}

.priceNone{ line-height:50px; font-size:16px; color:#999; text-align:center;}


/**/
.split_bottom{ display:block; width:100%; margin:50px 0 40px 0; overflow:hidden; text-align:center;}
.split_bottom img{ display:block; text-align:center; height:100%; width:auto; margin-left:-47%;}

/*2*/
.inlay_title{ margin:0; padding:0; overflow:hidden; margin:auto;}
.inlay_title li{ list-style:none; width:32.8%; float:left; padding-bottom:20px;}
.inlay_title li a{ display:block; width:100%; line-height:50px; background-color:#dbdbdb; text-align:center; font-size:20px; text-decoration:none; color:#333; border-radius:5px; -moz-border-radius:5px; -webkit-border-radius:5px;}
.inlay_title .inlay_titleClick{ background-size:40%; background-image:url("../img/diamond/arrow_white.png"/*tpa=http://m.yiko.org/mobile/img/diamond/arrow_white.png*/); background-position:bottom center; background-repeat:no-repeat;}
.inlay_title .inlay_titleClick a{ background-color:#fbe54d;}

.inlay_ring{ margin:0; padding:0; overflow:hidden; padding-left:10px;height: 500px;}
.ring{
    list-style: none;
    float: left;
    border: 1px solid #f0f0f0;
    width: 40.4%;
    overflow: hidden;
    margin-right: 10px;
    margin-bottom: 10px;
    overflow: hidden;
    position: relative;
    box-shadow: 0px 4px 8px #f3f3f3;
    padding: 0 6px 6px 6px;
    margin-left: 5px;
    margin-bottom: 16px;
}
.ring img{ display:block; width:100%; height:auto;}
.ringImg{ width:100%; height:100%; background-repeat:no-repeat; background-position:center; background-size:cover;}
.ring_info{ display:block; width:100%; height:100%; position:absolute; left:0; top:0; background-color:rgba(0,0,0,.5); text-decoration:none; display:none;}
.ring_info h1{ font-size:14px; color:#fff; text-align:center; margin:0; line-height:22px; height:22px; overflow:hidden; margin-bottom:6px; margin-top:14px; text-overflow:ellipsis; -o-text-overflow:ellipsis; white-space:nowrap; width:100%;}
.ring_info h2{ font-size:18px; color:#fff; text-align:center; margin:0;}
.ring_info a{ display:block; width:60%; line-height:36px; border:2px solid #fff; font-size:16px; color:#fff; margin:auto; text-align:center; margin-top:10px;}


.inlay_more{ display:block; width:80%; text-align:center; line-height:50px; background-color:#fbe65c; color:#553224; font-size:18px; margin:auto; margin-top:30px; text-decoration:none;}
.inlay_more:hover{ background-color:#d9d9d9; color:#333;}
.noMore{ display:block; text-align:center; font-size:14px; color:#999; margin-top:20px; display:none;}

/*3*/
.step_line{}
.stepLeft{ width:100%; margin-top:10px;}
.stepLeft img{ width:100%; height:auto;}
.stepRight{ width:100%;}
.stepRight_02,.stepRight_03{ font-size:14px; font-weight:bolder; color:#333; line-height:20px; margin:0;}
.stepRight_03{ font-weight:normal; margin:6px 0;}
.stepRight li{ list-style:inside; font-size:14px; color:#333; line-height:20px; margin-bottom:6px;}
.stepRight span{ font-size:14px; color:#666;}
.callNum{ margin-top:10px; font-size:24px; color:#333;}


/*关于我们*/
.aboutUs{ padding: 0 20px 50px;}
.aboutUs_left{ width:50%; margin:auto; margin-top:20px;}
.aboutUs_left img{ width:100%; height:auto;}
.aboutUs_right{ width:100%;}
.aboutUs_text{ font-size:14px; color:#333; line-height:24px; text-indent:2em;}




